{
    "version": 2,
    "routes": [
      { "src": "/check_stock", "dest": "/api/check_stock.php" },
      { "src": "/add_product", "dest": "/api/add_product.php" },
      { "src": "/dashboard", "dest": "/api/dashboard.php" },
      { "src": "/authenticate", "dest": "/api/authenticate.php" },
      { "src": "/deconnexion", "dest": "/api/deconnexion.php" },
      { "src": "/login", "dest": "/api/login.php" },
      { "src": "/logout", "dest": "/api/logout.php" },
      { "src": "/register", "dest": "/api/register.php" },
      { "src": "/remove_product", "dest": "/api/remove_product.php" },
      { "src": "/db.php", "dest": "/api/db.php" },
      { "src": "/(.*)", "dest": "/api/dashboard.php" }
    ],
    "functions": {
      "api/dashboard.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/db.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/check_stock.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/authenticate.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/add_product.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/remove_product.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/register.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/login.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/deconnexion.php": {
        "runtime": "vercel-php@0.4.0"
      },
      "api/logout.php": {
        "runtime": "vercel-php@0.4.0"
      }
    }
  }